<?xml version="1.0"?>
<uwsgi id="rsdn">
<app mountpoint="/"></app>
<module>deploy:application</module>
<socket>/tmp/uwsgi.sock</socket>
<processes>4</processes>
<chdir>/var/www/rsdn</chdir>
<pidfile>/tmp/rsdn.pid</pidfile>
<daemonize>/var/www/rsdn/log/rsdn-uwsgi.log</daemonize>
<stats>/tmp/rsdn.stats</stats>
<uid>uwsgi</uid>
<gid>www-data</gid>
<chmod-socket>775</chmod-socket>
</uwsgi>
